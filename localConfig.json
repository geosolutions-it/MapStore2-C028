                "priority": 2,
                "geomService" : {
                  "type": "wfs",
                  "options": {
                    "url": "/geoserver/wfs",
                    "typeName": "Cartografia:civici",
                    "srsName": "EPSG:4326",
                    "staticFilter": "ID = ${properties.code}"
                  }
                }
              }]
            }
          ]},
          "withToggle": ["max-width: 768px", "min-width: 768px"]
        }
    }, {
      "name": "Toolbar",
      "id": "NavigationBar",
      "cfg": {
        "id": "navigationBar"
      }
    }, "ZoomAll", {
      "name": "MapLoading",
      "override": {
        "Toolbar": {
          "alwaysVisible": true
