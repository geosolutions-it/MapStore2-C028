            "Map Service - Bolzano/Bozen": {
              "url": "/geoserver/wms",
              "type": "wms",
              "title": "Map Service - Bolzano/Bozen",
              "autoload": false
            }
          }
        },
        "newService": {
            "title": "",
            "type": "wms",
            "url": "",
            "autoload": false
        }
      }
    }
  },
