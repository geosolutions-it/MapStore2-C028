{
  "proxyUrl": {
    "url": "/mapstore3/proxy/?url=",
    "useCORS": ["http://nominatim.openstreetmap.org", "https://nominatim.openstreetmap.org"]
  },
  "geoStoreUrl": "rest/geostore/",
  "printUrl": "pdf/info.json",
  "mapquestApiKey": "__API_KEY_MAPQUEST__",
  "initialMapFilter": "",
  "ignoreMobileCss" : false,
  "useAuthenticationRules": true,
  "loadAfterTheme": true,
  "localizedLayerStyles": {
    "name": "mapstore_language"
  },
  "authenticationRules": [
    {
      "urlPattern": ".*geostore.*",
      "method": "bearer"
    }, {
      "urlPattern": "^\\/geoserver.*",
      "authkeyParamName": "ms2-authkey",
      "method": "authkey"
    }, {
      "urlPattern": ".*sit.comune.bolzano.it\\/geoserver.*",
      "authkeyParamName": "ms2-authkey",
      "method": "authkey"
  }],
  "monitorState": [
    {"name": "router", "path": "router.location.pathname"},
    {"name": "browser", "path": "browser"},
    {"name": "featuregridmode", "path": "featuregrid.mode"}],
  "projectionDefs": [{
    "code": "EPSG:25832",
    "def": "+proj=utm +zone=32 +ellps=GRS80 +units=m +no_defs",
    "extent": [265948.8191, 6421521.2254, 677786.3629, 7288831.7014],
    "worldExtent": [5.0500, 57.9000, 12.0000, 65.6700]
  }],
  "initialState": {
    "defaultState": {
      "annotations": {
        "config": {
          "multiGeometry": true,
          "validationErrors": {}
        },
        "format": "decimal",
        "defaultTextAnnotation": "New"
      },
      "maptype": {
        "mapType": "{context.mode === 'desktop' ? 'openlayers' : 'leaflet'}"
      },
      "locales": {
        "supportedLocales": {
          "it": {
            "code": "it-IT",
            "description": "Italiano"
          },
          "de": {
            "code": "de-DE",
            "description": "Deutsch"
          }
        }
      },
      "maps": {
        "mapType": "openlayers"
      },
      "mapInfo": {"enabled": true, "infoFormat": "text/html"},
      "catalog":{
        "default": {
          "newService": {
              "url": "",
              "type": "wms",
              "title": "",
              "isNew": true,
              "autoload": false
          },
          "selectedService": "Comune di Bolzano Test GeoServer",
          "services": {
            "Comune di Bolzano Test GeoServer": {
              "url": "/geoserver2/wms",
              "type": "wms",
              "title": "Comune di Bolzano Test GeoServer",
              "autoload": false
            }
          },
          "staticServices": {
            "default_map_backgrounds": {
              "type": "backgrounds",
              "title": "defaultMapBackgroundsServiceTitle",
              "autoload": true,
              "backgrounds": [{
                  "type": "osm",
                  "title": "Open Street Map",
                  "name": "mapnik",
                  "source": "osm",
                  "group": "background"
              }, {
                  "source": "ol",
                  "group": "background",
                  "title": "Empty Background",
                  "fixed": true,
                  "type": "empty"
              }]
            }
          }
        },
        "newService": {
            "title": "",
            "type": "wms",
            "url": "",
            "autoload": false
        }
      }
    }
  },
  "plugins": {
    "mobile": [{
        "name": "Map",
        "cfg": {
          "mapOptions": {
            "openlayers": {
              "interactions": {
                "pinchRotate": false,
                "altShiftDragRotate": false
              }
            }
          },
          "tools": ["locate"],
          "toolsOptions": {
            "measurement": {
              "metric": true
            }
          }
        }
      }, "DrawerMenu",
      {
        "name": "Identify",
        "showIn": ["Settings"],
        "cfg": {
            "showFullscreen": true,
            "position": "bottom",
            "size": 0.5,
            "fluid": true,
            "viewerOptions": {
                "container": "{context.ReactSwipe}"
            }
        }
    }, {
      "name": "Locate",
      "override": {
        "Toolbar": {
          "alwaysVisible": true
        }
      }
    }, "Home", {
      "name": "TOC",
      "cfg": {
        "activateMetedataTool": false,
        "activateMapTitle": false,
        "activateSortLayer": false,
        "activateLayerFilterTool": false,
        "activateQueryTool": false
      }
    }, {
      "name": "TOCItemsSettings",
      "cfg": {
          "width": 300
      }
  }, {
      "name": "BackgroundSelector",
      "cfg": {
        "thumbs":{
          "bolzano":{
            "Cartografia:ortofoto_2017": "assets/img/bolzano-2016.png",
            "Cartografia:ortofoto2014": "assets/img/bolzano-2014.png"
          }
        },
        "bottom": 25,
        "dimensions": {
          "side": 65,
          "sidePreview": 65,
          "frame": 3,
          "margin": 5,
          "label": false,
          "vertical": true
        }
      }
    }, {
      "name": "Settings",
      "cfg": {
        "wrap": true
      }
    },
    {
        "name": "Search",
        "cfg": {
          "showOptions": false,
          "resultsStyle": {
            "iconUrl": "https://cdn.rawgit.com/pointhi/leaflet-color-markers/master/img/marker-icon-red.png",
            "shadowUrl": "https://cdnjs.cloudflare.com/ajax/libs/leaflet/0.7.7/images/marker-shadow.png",
            "iconSize": [25, 41],
            "iconAnchor": [12, 41],
            "popupAnchor": [1, -34],
            "shadowSize": [41, 41],
            "color": "#3388ff",
            "weight": 4,
            "dashArray": "",
            "fillColor": "#3388ff",
            "fillOpacity": 0.2
          },
          "searchOptions": {"services": [
            {
              "type": "bzVie",
              "displayName": "${properties.desc}",
              "subTitle": "Bolzano / Bozen",
              "options": {
                "pathname": "/GeoInfo/VieServlet",
                "lang": "it"
              },
              "priority": 1,
              "nestedPlaceholderMsgId": "search.nestedPlaceholder",
              "geomService" : {
                "type": "wfs",
                "options": {
                  "url": "/geoserver2/wfs",
                  "typeName": "ctn_base:grafo_vie",
                  "srsName": "EPSG:4326",
                  "staticFilter": "ID_STRASSE = ${properties.code}"
                }
              },
              "then" : [
                {
                  "type": "bzCivico",
                  "searchTextTemplate": "${properties.desc}",
                  "displayName": "${properties.desc}",
                  "subTitle": "Bolzano / Bozen",
                  "options": {
                    "pathname": "/GeoInfo/CiviciServlet",
                    "idVia": "${properties.code}"
                  },
                "priority": 2,
                "geomService" : {
                  "type": "wfs",
                  "options": {
                    "url": "/geoserver2/wfs",
                    "typeName": "Cartografia:civici",
                    "srsName": "EPSG:4326",
                    "staticFilter": "ID = ${properties.code}"
                  }
                }
              }]
            }
          ]},
          "withToggle": ["max-width: 768px", "min-width: 768px"]
        }
    }, {
      "name": "Toolbar",
      "id": "NavigationBar",
      "cfg": {
        "id": "navigationBar"
      }
    }, "ZoomAll", {
      "name": "MapLoading",
      "override": {
        "Toolbar": {
          "alwaysVisible": true
        }
      }
    }, "Login",
    "OmniBar", "BurgerMenu", "Expander",
    {
      "name": "Cookie",
      "cfg": {
        "externalCookieUrl" : "",
        "declineUrl" : "http://www.google.com"
      }
    },
    "FeedbackMask",
    "AddGroup"
  ],
